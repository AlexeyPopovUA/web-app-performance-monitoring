    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.846.0",
    "@aws-sdk/client-sfn": "^3.847.0",
    "@aws-sdk/client-sqs": "^3.846.0",
    "@codegenie/serverless-express": "^4.17.0",
    "@types/aws-lambda": "^8.10.150",
    "aws-cdk-lib": "2.206.0",
    "constructs": "^10.4.2",
    "cors": "^2.8.5",
